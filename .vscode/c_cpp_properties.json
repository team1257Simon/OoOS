{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${default}",
                "${workspaceFolder}/common/include/c++",
                "${workspaceFolder}/common/include",
                "${workspaceFolder}/common/include/kernel",
                "${workspaceFolder}/boot/uefi",
                "/usr/local/ooos_sysroot/usr/include"
            ],
            "defines": [
                "__STDC_HOSTED__=0"
            ],
            "compilerPath": "/usr/local/bin/x86_64-elf-gcc",
            "intelliSenseMode": "linux-gcc-x64",
            "cppStandard": "c++26",
            "cStandard": "c23",
            "compilerArgs": [
                "-D__KERNEL__"
            ],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}